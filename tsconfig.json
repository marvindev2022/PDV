{
  "compilerOptions": {
    "target": "ES2020",
    "module": "commonjs",
    "outDir": "PDV/dist",
    "rootDir": "./src",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "declaration": true
  },
  "include": ["src/**/*.ts", "src/Router/router.test.js"],
  "exclude": ["node_modules"]
}
